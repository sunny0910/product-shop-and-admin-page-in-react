{
  "name": "product-shop-and-checkout-page-in-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install_app": "concurrently \"npm run-script install_server\" \"npm run-script install_client\" ",
    "install_server": "npm install",
    "install_client": "cd client && npm install",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\" "
  },
  "dependencies": {
    "aws-sdk": "^2.401.0",
    "bcrypt": "^3.0.4",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.11",
    "morgan": "^1.9.1",
    "node-gyp": "^3.8.0",
    "nodemon": "^1.18.10",
    "npm": "^6.11.3"
  }
}
